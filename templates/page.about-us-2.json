    "3a294ada-62cf-4372-8f14-f54c1c7099cf": {
      "type": "image-with-text",
      "blocks": {
        "template--15891164659862__3a294ada-62cf-4372-8f14-f54c1c7099cf-1669440721f8521bb3-0": {
