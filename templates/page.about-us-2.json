    "9f2b6154-1a5f-4900-b5a9-3e61e6ee0c72": {
      "type": "image-with-text",
      "blocks": {
        "template--15891164659862__9f2b6154-1a5f-4900-b5a9-3e61e6ee0c72-1669444564eb4f633f-0": {
          "type": "heading",
