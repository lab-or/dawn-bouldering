    "85a68067-9aa9-4054-8e76-b4c583266bd4": {
      "type": "image-with-text",
      "blocks": {
        "template--15891164659862__85a68067-9aa9-4054-8e76-b4c583266bd4-1669444649cf4f790b-0": {
