    "c81aa356-caf7-4648-9ded-5a6304d1e729": {
      "type": "image-with-text",
      "blocks": {
        "template--15867959214230__c81aa356-caf7-4648-9ded-5a6304d1e729-1667311110d43d4e26-0": {
          "type": "heading",
